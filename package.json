{
  "name": "jeweller",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:frontend": "tsc && vite build",
    "build:backend": "cd server && npm install",
    "build:all": "npm install && npm run build:frontend && npm run build:backend",
